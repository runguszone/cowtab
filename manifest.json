{
   "background": {
      "persistent": false,
      "scripts": [ "libraries/private.js", "background.js" ]
   },
   "chrome_url_overrides": {
      "newtab": "newtab/newtab.html"
   },
   "default_locale": "en",
   "description": "A simple, fast, new tab page, with a retro feel and good advice.",
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
   "manifest_version": 2,
   "name": "cow tab",
   "options_ui": {
      "chrome_style": true,
      "page": "options/options.html"
   },
   "permissions": [ "management", "storage" ],
   "version": "1.3"
}
